<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>msf4j-service</artifactId>
    <groupId>org.wso2.msf4j</groupId>
    <version>2.6.2</version>
    <relativePath>../pom.xml/pom.xml</relativePath>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>br.com.godinhowolff</groupId>
  <artifactId>avaliacao-backend</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <build>
    <sourceDirectory>src</sourceDirectory>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <properties>
    <postgresql.version>9.4.1212</postgresql.version>
    <gson.version>2.8.0</gson.version>
    <hibernate.version>5.0.1.Final</hibernate.version>
    <c3p0.version>5.0.1.Final</c3p0.version>
    <microservice.mainClass>br.com.godinhowolff.application.Application</microservice.mainClass>
  </properties>
</project>

